#!python
cubit.cmd('set node constraint on')
cubit.cmd('set hdf5_cub_file on')
cubit.cmd('reset')
cubit.cmd('create surface rectangle width 1.6 height 0.8 zplane')
cubit.cmd('surface 1 size 0.2')
cubit.cmd('mesh surface 1')
cubit.cmd('block 1 add face all')
cubit.cmd('block 1  element type shell9')
cubit.cmd('nodeset 1 add node al in curve all')
cubit.cmd('reset')
cubit.cmd('create surface rectangle width 1.6 height 0.8 zplane')
cubit.cmd('surface 1 size 0.1')
cubit.cmd('mesh surface 1')
cubit.cmd('block 1 add face all')
cubit.cmd('block 1  element type shell9')
cubit.cmd('nodeset 1 add node al in curve all')
